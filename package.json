{
  "name": "pfe-lab",
  "version": "0.0.1",
  "description": "React/Redux JS application for the Zooniverse Lab",
  "main": "./src/index.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/zooniverse/pfe-lab"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "ES6",
    "ES2015",
    "babel",
    "mocha",
    "chai",
    "zooniverse"
  ],
  "scripts": {
    "start": "export NODE_ENV=development; check-engines && check-dependencies && webpack-dashboard -p 3777  -- babel-node ./server.js",
    "_build": "export HEAD_COMMIT=$(git rev-parse --short HEAD); check-engines && check-dependencies && rimraf dist; webpack --config ./webpack.production.config.js -p",
    "_publish": "publisssh ./dist \"zooniverse-static/$PATH_ROOT/$SUBDIR\"",
    "_log-staging-url": "echo \"Staged at https://$SUBDIR.lab-preview.zooniverse.org/\"",
    "_stage": "export PATH_ROOT=preview.zooniverse.org/pfe-lab; npm run _build && npm run _publish && npm run _log-staging-url",
    "stage": "export NODE_ENV=staging; export SUBDIR=$(git symbolic-ref --short HEAD | awk '{print tolower($0)}'); npm run _stage",
    "_deploy": "export PATH_ROOT=lab.zooniverse.org; npm run _build && npm run _publish",
    "deploy": "export NODE_ENV=production; npm run _deploy",
    "eslint": "eslint .",
    "test": "NODE_ENV=development BABEL_ENV=test mocha"
  },
  "engines": {
    "node": ">=8.1.2",
    "npm": ">=5.3.0"
  },
  "author": "Zooniverse",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zooniverse/pfe-lab"
  },
  "homepage": "https://github.com/zooniverse/pfe-lab",
  "dependencies": {
    "babel-plugin-rewire": "~1.1.0",
    "classnames": "~2.2.5",
    "counterpart": "~0.18.0",
    "data-uri-to-blob": "~0.0.4",
    "dompurify": "~1.0.9",
    "drag-reorderable": "~0.1.0",
    "grommet": "~1.13.0",
    "history": "~4.4.1",
    "markdownz": "~7.6.3",
    "panoptes-client": "~2.11.2",
    "prop-types": "~15.5.10",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-redux": "~5.1.1",
    "react-router": "~4.3.1",
    "react-select": "~1.0.0-rc.3",
    "redux": "~3.6.0",
    "redux-devtools-extension": "~1.0.0",
    "redux-thunk": "~2.1.0",
    "zoo-grommet": "~0.3.0",
    "zooniverse-react-components": "github:zooniverse/Zooniverse-React-Components#v0.5.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.4.1",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.16.0",
    "chai": "~3.5.0",
    "check-dependencies": "~1.0.1",
    "check-engines": "~1.5.0",
    "clean-webpack-plugin": "~1.0.1",
    "css-loader": "~0.28.0",
    "enzyme": "~2.7.0",
    "eslint": "~3.11.1",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-loader": "~1.6.1",
    "eslint-plugin-import": "~2.12.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.10.3",
    "eventsource-polyfill": "~0.9.6",
    "express": "~4.16.3",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.11.1",
    "html-webpack-plugin": "~2.28.0",
    "image-webpack-loader": "~4.6.0",
    "jsdom": "~12.0.0",
    "mocha": "~5.2.0",
    "nib": "~1.1.2",
    "nock": "~9.3.3",
    "node-sass": "~4.9.3",
    "publisssh": "~1.1.0",
    "react-addons-test-utils": "~15.4.1",
    "redux-mock-store": "~1.5.3",
    "rimraf": "~2.5.4",
    "sass-loader": "^6.0.5",
    "sinon": "~7.2.3",
    "stats-webpack-plugin": "~0.7.0",
    "style-loader": "~0.13.1",
    "stylus": "~0.54.5",
    "stylus-loader": "~2.4.0",
    "webpack": "~2.4.1",
    "webpack-dashboard": "~2.0.0",
    "webpack-dev-middleware": "~1.10.2",
    "webpack-hot-middleware": "~2.18.0",
    "webpack-split-by-path": "~2.0.0"
  }
}
